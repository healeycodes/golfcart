i = 0
for j = 0; j < 5; j = j + 1 {
    assert(i, j)
    i = i + 1
}

for nil;false ;nil  {
    // Doesn't run
    assert(true, false)
}

for false {
    // Doesn't run
    assert(true, false)
}

for true {
    break
    // Doesn't run
    assert(true, false)
}

// Implicit infinite loop
for { break }

for i = 0; i < 1; i = i + 1 {
    continue
    // Doesn't run
    assert(true, false)
}