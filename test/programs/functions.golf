counter = () => {
    n = 0
    () => {
        n = n + 1
        n
    }
}

my_counter = counter()
my_counter()
my_counter()
my_counter()

a = counter
b = counter
assert(a != b, true)

last_value_return = () => {
    1
    2
}
assert(last_value_return(), 2)

with_return = () => {
    return 1

    // This won't evaluate
    assert(true, false)
}
assert(with_return(), 1)