a = [1, 2]
[]
[3, 4]
a[0]
b = a[0]
c = a[0]
assert(b, 1)
assert(b, c)

assert([5] == [5], false)
assert([] == [], false)

d = [1]
assert(d[0] + 1 == 2, true)

e = [0, 1]
f = [2, 3]
g = e + f
for i = 0; i < 4; i = i + 1 {
    assert(g[i], i)
}

h = [0]
h.append(-1)
assert(h[1], -1)

j = [5]
j[0] = 6
assert(j[0], 6)

l = [0]
l.prepend(-1)
assert(l[0], -1)
assert(l[1], 0)

m = ["a", "b"]
n = m.pop()
assert(n, "b")
assert(len(l), 1)

o = ["a", "b"]
p = m.pop_left()
assert(p, "a")
assert(len(o), 1)